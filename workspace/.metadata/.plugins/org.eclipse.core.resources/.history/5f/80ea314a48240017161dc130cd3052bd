package UniqueNumbers;

public class operation {
	public int process(int a, int b, int c){
		if (a == b && b == c){
			return 0;
		}
		else if (a == b && a != c){
			return c;
		}
		else if (a != b && a == c){
			return b;
		}
		else if (a != c && b == c){
			return a;
		}
		else{
			return a+b+c;
		}
	}

	public static void main(String[] args) {
		operation O = new operation();
		System.out.println(O.process(1,2,3));
		System.out.println(O.process(3,3,3));
		System.out.println(O.process(1,1,3));
	}

}
