import java.util.*;

public class Paint {

	public static void main(String[] args) {
		Cheapo C = new Cheapo();
		Average A = new Average();
		Dulux D = new Dulux();
		C.Order(19.99f,20,10);
		A.Order(17.99f,15,11);
		D.Order(25,10,20);
		ArrayList<Holding> arr = new ArrayList<Holding>();
		arr.add(C);
		arr.add(A);
		arr.add(D);
		for (Holding i : arr) {
			i.count = 220/i.area;
			System.out.println(i.count);
		}
	}
}