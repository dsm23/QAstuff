package FileNonsense;

import java.io.*;
import java.util.*;

public class ReadingScan {

	public static void main(String[] args) {
		ArrayList<Person> readarr = new ArrayList<Person>();
		String[] temp = String[3];
		
		Person David = new Person();
		Person James = new Person();
		Person Emily = new Person();
		Person John = new Person();
		Person Grace = new Person();
		
		readarr.add(David);
		readarr.add(James);
		readarr.add(Emily);
		readarr.add(John);
		readarr.add(Grace);
		
		try {
			int y=0;
		  File x = new File("test.txt");
		  Scanner sc = new Scanner(x);

		  while(sc.hasNext()) {
			    String line = sc.nextLine();
			    System.out.println(line);
			    temp = line.split(" ");
			    for (Person i : readarr){
					i.process(temp[0], temp[1], (int)temp[2];
				}
		  }
		  sc.close();
		  System.out.println(y);
		}
		 catch (FileNotFoundException e) {
			 System.out.println("Error");
		}
	}
}